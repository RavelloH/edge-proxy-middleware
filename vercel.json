{
    "rewrites": [
        {
            "source": "/:protocol(https|http)://:path*",
            "destination": "/api/edge?url=:protocol://:path*"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}
